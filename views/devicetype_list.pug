extends layout

block content
   div(class="col-sm-7 my-5")
      h1= title

      ul 
         each devicetype in devicetype_list
            li
               a(href=devicetype.url) #{devicetype.name}
         else
            li Brak typów urządzeń.
   
   div(class="col-lg-2 my-5")
      a(href="/api/devicetype/create") Utwórz Typ Urządzenia